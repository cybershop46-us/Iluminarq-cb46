<odoo>
  <data>
    <record id="view_project_form" model="ir.ui.view">
        <field name="name">project.form.interior.product</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="action_view_internal_products" type="object" string="Internal Products" class="oe_stat_button" icon="fa-cogs" />
            </xpath>
        </field>
    </record>
  </data>
</odoo>